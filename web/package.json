{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "create-test-db": "docker run --rm -d --name nextit-web-postgres -p 5432:5432 -e POSTGRES_PASSWORD=postgres postgres:16-alpine",
    "remove-test-db": "docker stop nextit-web-postgres",
    "generate-proto": "protoc-gen-grpc --js_out=import_style=commonjs,binary:./ --grpc_out=grpc_js:./ proto/item.proto && protoc-gen-grpc-ts --ts_out=grpc_js:. proto/item.proto"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.11",
    "@grpc/grpc-js": "^1.9.13",
    "@grpc/proto-loader": "^0.7.10",
    "@prisma/client": "^5.7.0",
    "bootstrap": "^5.3.2",
    "google-protobuf": "^3.21.2",
    "grpc-promise": "^1.4.0",
    "next": "14.0.4",
    "next-auth": "^4.24.5",
    "react": "^18",
    "react-dom": "^18",
    "sharp": "^0.33.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "prisma": "^5.7.0",
    "sass": "^1.69.5",
    "tailwindcss": "^3.3.0"
  }
}
